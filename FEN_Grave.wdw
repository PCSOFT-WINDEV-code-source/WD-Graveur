   -
     name : GravureProgression
     internal_properties : CAAAAAgAAADSMn7Rs1oeIZRePbCBQBOv/H+CoHSuktJHMowV8vd8bcL/L6Mm1TWmyONM6loZzUjJcS4C2+uo2WLjikBO3tA+dOXmwoLQJM5tn79s4gN1GYRidy47Axi2Xm3VFGwrrK4SVHg3NgrljbEbijSQJgItAi22UvYuztaTa4atB/nuqamlHkAv/smsrMXsd26kczU3XUi1nqMk2ShphJwOiYVBSIlgnAIDpcvlVIkwcU0GpPSu34F9NhogYzfWTMVA2cJV
     procedure_id : 1449724420742093920
     type_code : 14
     code : |1+
      // Syntaxe :
      //GravureProgression (<nIDNotification> est entier, <sMessage> est chaîne, <nParamPerso> est entier, <nParam1> est entier, <nParam2> est entier)
      //
      // Paramètres :
      //	nIDNotification (entier) : Identifiant de la notification gravure
      //	sMessage (chaîne ANSI) : Message texte complémentaire de la notification
      //	nParamPerso (entier) : Entier passé en paramètre sur GravureEnregistre (si un entier a été indiqué)
      //	nParam1 (entier) : Entier, dépend de la notification (donc de nIDNotification)
      //	nParam2 (entier) : Entier, dépend de la notification (donc de nIDNotification)
      
      Procédure GravureProgression(nIDNotification est un entier, sMessage est une chaîne, nParamPerso est un entier, nParam1 est un entier, nParam2 est un entier)
      
      // Cette CallBack est générique et peut être passée en paramètre à GraveurEnregistre et GraveurEfface (Rappel : vous pouvez aussi utiliser des callbacks différentes pour chaque fonction)
      // Les paramètres sMessage, nParam1 et nParam2 ne sont pas utilisés pour tous les évènements. Si un paramètre n'est pas utilisé, il est initialisé à "" ou 0.
      
      SELON  nIDNotification
      	CAS GraveurNotificationAbandon:
      		listeajoute(LISTE_InfoGravure,gimage(IMG_erreur)+" Abandon de la gravure")
      		ListeSelectPlus(LISTE_InfoGravure,ListeOccurrence(LISTE_InfoGravure))
      		
      		// Déclenche une erreur
      		erreurdéclenche(1,"Abandon de la gravure")
      		
      		Ferme(FEN_Grave)
      		
      	CAS GraveurNotificationErreur:
      		// nParam1 : contient le code d'erreur utilisateur
      		listeajoute(LISTE_InfoGravure,gimage(IMG_erreur)+" Une erreur a été détectée pendant la gravure."+nParamPerso+glien(sMessage))
      		ListeSelectPlus(LISTE_InfoGravure,ListeOccurrence(LISTE_InfoGravure))
      		
      		ErreurDéclenche(1,"Une erreur a été détectée pendant la gravure."+nParamPerso+RC+sMessage)
      		Ferme(FEN_Grave)
      		
      			
      	CAS GraveurNotificationFermetureCD:
      		// nParam1 : Durée estimée en secondes		
      		listeajoute(LISTE_InfoGravure,gimage(IMG_info)+" Fermeture du disque. Temps estimé = " + nParam1 + " secondes. ")
      		ListeSelectPlus(LISTE_InfoGravure,ListeOccurrence(LISTE_InfoGravure))
      		
      	
      	CAS GraveurNotificationFinEffacement:
      		// nParam1 : Etat du graveur. 0 si aucune erreur.
      		si nParam1 <> 0 ALORS
      			
      			ErreurDéclenche(1,ChaîneConstruit("Effacement du disque terminé anormalement (%1) : %2",nParam1,sMessage))
      
      			listeajoute(LISTE_InfoGravure,gimage(IMG_erreur)+ChaîneConstruit(" Effacement du disque terminé anormalement (%1) : %2",nParam1,sMessage))
      			ListeSelectPlus(LISTE_InfoGravure,ListeOccurrence(LISTE_InfoGravure))
      
      		FIN
      		
      		ferme(FEN_Grave)
      	
      	CAS GraveurNotificationFinGravure:
      		// nParam1 : Etat du graveur. 0 si aucune erreur.
      		si nParam1 = 0 alors	
      			
      			listeajoute(LISTE_InfoGravure,gimage(IMG_info)+" Notification de fin de Gravure")	
      			ListeSelectPlus(LISTE_InfoGravure,ListeOccurrence(LISTE_InfoGravure))	
      			
      			BTN_Graver..etat =actif
      			
      			// Initialisation des jauges
      			FEN_Grave.JAUGE_BLOCS			= nParam1
      			FEN_Grave.JAUGE_BLOCS..Libellé 	= "Bloc n° : "+nParam1+" /"+nParam2
      			FEN_Grave.JAUGE_PISTES			= nParam1
      			FEN_Grave.JAUGE_PISTES..Libellé = "Piste n° : "+nParam1+" /"+nParam2
      
      			
      
      		sinon
      			listeajoute(LISTE_InfoGravure,gimage(IMG_erreur)+ChaîneConstruit(" Gravure du disque terminé anormalement (%1) : %2",nParam1,sMessage))
      			ListeSelectPlus(LISTE_InfoGravure,ListeOccurrence(LISTE_InfoGravure))
      			
      			ErreurDéclenche(1,ChaîneConstruit("Gravure du disque terminé anormalement (%1) : %2",nParam1,sMessage))
      			
      		fin
      		
      		Ferme(FEN_Grave)
      		
      	CAS GraveurNotificationGravureBloc:
      		// nParam1 : Bloc actuel
      		// nParam2 : Nombre total de blocs
      		si FEN_Grave.JAUGE_BLOCS..BorneMax<>nParam2 alors FEN_Grave.JAUGE_BLOCS..BorneMax=nParam2
      		// Remplissage jauge et affichage du bloc en cours
      		FEN_Grave.JAUGE_BLOCS			= nParam1
      		FEN_Grave.JAUGE_BLOCS..Libellé 	= "Bloc n° : "+nParam1+" /"+nParam2
      		si pas FEN_Grave.JAUGE_BLOCS..visible=vrai alors FEN_Grave.JAUGE_BLOCS..visible=vrai
      		
      		
      	CAS GraveurNotificationGravurePiste: // lors de la gravure d'un CD Audio
      		// nParam1 : Piste actuelle
      		// nParam2 : Nombre total de Piste
      		si FEN_Grave.JAUGE_PISTES..BorneMax <> nParam2 alors 
      			FEN_Grave.JAUGE_PISTES..BorneMax = nParam2
      		FIN
      		
      		// Remplissage jauge et affichage de la piste en cours
      		FEN_Grave.JAUGE_PISTES			= nParam1
      		FEN_Grave.JAUGE_PISTES..Libellé = "Piste n° : "+nParam1+" /"+nParam2
      		
      		si pas FEN_Grave.JAUGE_PISTES..visible=vrai alors 
      			FEN_Grave.JAUGE_PISTES..visible=vrai
      		FIN	
      		
      	CAS GraveurNotificationPrepareGravure:
      		// nParam1 : Durée estimée en secondes	
      		listeajoute(LISTE_InfoGravure,gimage(IMG_info)+" Préparation de la gravure. Temps estimé = " + nParam1 + " secondes.")
      		ListeSelectPlus(LISTE_InfoGravure,ListeOccurrence(LISTE_InfoGravure))
      				
      	AUTRE CAS:
      		//au cas où...
      		listeajoute(LISTE_InfoGravure,gimage(IMG_Attention)+" Statut inconnu = " + nIDNotification + "  (" + nParam1 +" / " + nParam2 + ")"+rc+ sMessage)	
      		
      FIN
     type : 458752
   -
     name : AjouteFichierAlaGravure
     procedure_id : 1450962904623458191
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //AjouteFichierAlaGravure (<sPListeDesFichiers>)
      //
      // Paramètres :
      //	sPListeDesFichiers : liste des fichiers à ajouter. le format attendu est : <NomEtCheminDuFichier1>;<CheminSurLeCD>RC<NomEtCheminDuFichier2>;<CheminSurLeCD>
      // Valeur de retour :
      // 	Aucune
      //
      Procédure AjouteFichierAlaGravure(sPListeDesFichiers)
      
      sfic 			est une chaine			// Variable de boucle
      sCheminFic 		est une chaine			// Nom et chemin d'accès au fichier
      sCheminSurCDDVD est une chaine			// Nom et Chemin d'accès sur le CD/DVD
      nSnbFic 		est un entier			// Compteur du Nombre de fichier Ajouté
      
      // Rend visible le libelle d'information
      LIB_InfoFichier..Visible = vrai
      // Affichage de l'information
      ListeAjoute(LISTE_InfoGravure,gImage(IMG_info)+" "+HeureVersChaîne(HeureSys(),"HH:MM:SS")+" Préparation des fichiers à la gravure")
      // On rend la main au système
      Multitâche(-1)
      // grise le bouton graver tant que tous les fichiers non pas été ajouté
      BTN_Graver..etat = Grisé
      POUR TOUTE CHAINE sfic DE sPListeDesFichiers SEPAREE PAR rc
      	// Chemin du fichier à graver
      	sCheminFic 		= ExtraitChaîne(sfic,1,";")
      	// Chemin sur le CD/DVD
      	sCheminSurCDDVD 	= ExtraitChaîne(sfic,2,";")
      	// Ajout à la liste des fichiers à graver
      	si pas GraveurAjouteFichier(sCheminFic,sCheminSurCDDVD) ALORS
      		Multitâche()
      		si sansespace(sCheminFic) <>"" alors
      			ListeAjoute(LISTE_InfoGravure,gImage(IMG_erreur)+" "+HeureVersChaîne(HeureSys(),"HH:MM:SS")+" Le fichier "+sCheminFic+" n'a pas été ajouté"+glien(erreurinfo(errMessage)))
      			ListeSelectplus(LISTE_InfoGravure,ListeOccurrence(LISTE_InfoGravure))
      		FIN
      	sinon
      		// comptage des fichiers ajouter
      		nSnbFic ++
      		LIB_InfoFichier = "Ajout du fichier : "+fExtraitChemin(sCheminFic,fFichier+fExtension)
      	FIN
      FIN
      // Affichage de l'information
      ListeAjoute(LISTE_InfoGravure,gImage(IMG_info)+" "+HeureVersChaîne(HeureSys(),"HH:MM:SS")+" Fin de la préparation")
      ListeAjoute(LISTE_InfoGravure,gImage(IMG_info)+" "+HeureVersChaîne(HeureSys(),"HH:MM:SS")+" "+nSnbFic+" fichiers ont été ajoutés")
      ListeSelectplus(LISTE_InfoGravure,ListeOccurrence(LISTE_InfoGravure))
      // Rend Invisible le libelle d'information
      LIB_InfoFichier..Visible = faux
      // Rend le bouton graver accessible
      BTN_Graver..etat = Actif
      
     type : 458752
