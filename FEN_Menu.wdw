  procedures :
   -
     name : AnnuleEffetLoupe
     procedure_id : 1683625848606064267
     type_code : 14
     code : |1+
      // Syntaxe :
      //AnnuleEffetLoupe (<Bouton>)
      //
      // Paramètres :
      //	Bouton : Champ bouton dont l'effet loupe doit être annulé
      // Valeur de retour :
      // 	Aucune
      //
      Procédure AnnuleEffetLoupe(Bouton)
      
      // Variables
      x est un entier
      nx est un entier = Bouton..x
      
      //
      // Réduit la taille et les dimensions du bouton
      //
      si Bouton..largeur <> 80 alors
      
      	pour x = 80 _a_ 128 pas 2
      		Bouton..x += 1
      		Bouton..y += 1
      		Bouton..largeur -= 2
      		Bouton..hauteur -= 2
      	FIN
      	
      	Bouton..largeur = 80
      	Bouton..hauteur = 80	
      
      fin
      
     type : 458752
   -
     name : AnnuleEffetLoupeTous
     procedure_id : 1683625848606129803
     type_code : 14
     code : |1+
      // Syntaxe :
      //AnnuleEffetLoupeTous ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      //	Sauf (valeur par défaut="") : Optionnel, Nom du champ bouton dont l'effet loupe ne doit pas être annulé
      Procédure AnnuleEffetLoupeTous()
      
      	i est un entier
      
      	pour i=1 _a_ NOMBRE_OPTIONS
      		si moimeme..nom <> "OPTION" + i alors
      			AnnuleEffetLoupe({"OPTION" + i, indchamp})
      		FIN
      	FIN
      
      
     type : 458752
   -
     name : EffetLoupe
     procedure_id : 1683625848606195339
     type_code : 14
     code : |1+
      // Syntaxe :
      //EffetLoupe ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      Procédure EffetLoupe()
      
      // Réinitialise l'effet loupe
      AnnuleEffetLoupeTous()
      
      x est un entier
      nx est un entier = moimeme..x
      
      
      //
      // Augmente la taille et les dimensions du bouton
      //
      si moimeme..largeur <> 128 alors
      	si fenetat(FEN_Bulle) <> inexistant alors
      		ferme(FEN_Bulle)
      	fin	
      
      	pour x = 80 a 128 pas 2
      		moimeme..x -= 1
      		moimeme..y -= 1
      		moimeme..largeur += 2
      		moimeme..hauteur += 2
      	FIN
      	
      	moimeme..largeur = 128
      	moimeme..hauteur = 128
      
      	ouvrefille("FEN_Bulle"+sansfocus, mafenetre..x + moimeme..x, mafenetre..Y + moimeme..y + moimeme..hauteur + 10, moimeme..largeur, moimeme..note)
      fin
      
      
