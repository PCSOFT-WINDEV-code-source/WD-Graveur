     type_code : 14
     code : |1-
      // Syntaxe :
      //PositionCurseur ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      Procédure PositionCurseur()
      
      	fintimer()
      
      	nX, nY sont des entiers
      	nX = poidsfaible(curseurpos(cpEcran))
      	nY = poidsfort(curseurpos(cpEcran))
      	
      	// Si la souris ne survole plus la fenetre
      	si (mafenetre..x < nX < mafenetre..x + mafenetre..largeur) = faux ou ...
      	   (mafenetre..y < nY < mafenetre..y + mafenetre..hauteur) = faux alors
      	   
      		i est un entier
      	
      		// Réinitialise l'effet loupe pour tous
      		pour i=1 _a_ NOMBRE_OPTIONS
      			AnnuleEffetLoupe({"OPTION" + i, indchamp})
      		FIN	   
      		
      		// Ferme la bulle
      		si fenetat(FEN_Bulle) <> inexistant alors
      			ferme(FEN_Bulle)
      		fin			
      	   
      	fin
      	
      	Timer(PositionCurseur, 5)
     type : 458752
