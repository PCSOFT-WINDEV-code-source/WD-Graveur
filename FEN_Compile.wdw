     name : _ListeFicEtRepAajouter
     internal_properties : 
     procedure_id : 1448583475557431066
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //_ListeFicEtRepAajouter (<pChemin>, <pNomdufichier>, <pChange> [, <pPointeur>])
      //
      // Paramètres :
      // 	pChemin : <indiquez ici le rôle de pChemin>
      // 	pNomdufichier : <indiquez ici le rôle de pNomdufichier>
      // 	pChange : <indiquez ici le rôle de pChange>
      // 	pPointeur (valeur par défaut=0) : < indiquez ici le rôle de pPointeur >
      // Valeur de retour :
      // 	Aucune
      //
      // Procédure CallBAck
      Procédure _ListeFicEtRepAajouter(pChemin, pNomdufichier, pChange, pPointeur=0)
      
      sChem est une chaine // chemin du fichier
      
      // Ajout des fichier et rep Trouvé
      si pChange = flChangeRépertoire ALORS //
      	// ajout d'un nouveau répertoire
      	gxNbDeRepSurCD_DVD++
      	//Récupération du chemin a afficher dans la branche
      	sChem = pChemin[[taille(gsCheminDeBase) a]]
      	sChem = Remplace(sChem,"\",tab)
      	// Construction de la branche de l'arbre pour le nouveau répertoire.
      	gsBrancheEnCours = gsBranche+Milieu(sChem,1,Taille(sChem)-1)
      	// Ajout de la branche à l'arbre
      	arbreajoute(ARBRE_Arbre2,gsBrancheEnCours,aDéfaut,aDéfaut,pChemin)
      FIN
      // ajout d'un fichier
      gxNbDeFichierSurCD_DVD++
      // Ajout dans le fichier de données de l'arborescence.
      hraz(FichierGrav)
      FichierGrav.CheminArbre 	= gsBrancheEnCours
      FichierGrav.CheminComplet 	= pChemin+pNomdufichier
      FichierGrav.DateFic       	= fdate(pChemin+pNomdufichier,"",fModification)
      FichierGrav.Nomfichier    	= fExtraitChemin(pChemin+pNomdufichier,fFichier)
      FichierGrav.TailleFic	 	= ftaille(pChemin+pNomdufichier)
      FichierGrav.Exten		  	= fExtraitChemin(pChemin+pNomdufichier,fExtension)
      hajoute(FichierGrav)
      // ajout du Nombre D'octets estimé sur futur CD/DVD
      gxNbOctetEstimé += FichierGrav.TailleFic
      
     type : 458752
