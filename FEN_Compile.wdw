     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //_intFenCompile ( [<pTypeCompile> est entier])
      //
      // Paramètres :
      //	pTypeCompile (entier - valeur par défaut=2) : Indiquer le type de compile : CompileDonnées ou CompileAudio
      // Valeur de retour :
      // 	Aucune
      //
      // Infos
      // Un bloc correspond à 2048 octets sur un CD/DVD de type donnée, à 2352 octets sur un CD de type audio
      Procédure _intFenCompile(pTypeCompile est un entier = CompileDonnées )
      
      
      //Par Défaut le nom de la compile
      SAI_Nom_du_CD_DVD = gNomCompile // Compile1
      
      selon pTypeCompile
      	cas CompileDonnées // Compile de données
      		//Affichage du plan approprié
      		FEN_Compile..plan = 1
      		si GraveurEtat() = graveurInactif ALORS
      			xCapacitétotal     = (GraveurInfoMédia(médiaTailleTotale)*0n2)/1024
      			xEspacelibre	   = (GraveurInfoMédia(médiaTailleLibre)*0n2)/1204
      			xEspaceUtilisé	   = (GraveurInfoMédia(médiaTailleUtilisée)*0n2)/1024
      		FIN
      		si xEspaceUtilisé > 0 alors
      			LectureDesDonnéesDuDisque()
      		FIN
      		CBM_NbRepFic	   = "0 Répertoire et 0 Fichier"
      		// On défini l'exention des fichiers filtrés.
      		gsExtCh = "*.*"
      	cas CompileAudio // Compile Audio
      		//Affichage du plan approprié
      		FEN_Compile..plan = 2
      		si GraveurEtat() = graveurInactif ALORS
      			xCapacitétotal     = ((GraveurInfoMédia(médiaTailleTotale)*0n2352)/1024)/1204
      			xEspacelibre	   = ((GraveurInfoMédia(médiaTailleLibre)*0n2352)/1024)/1024
      			xEspaceUtilisé	   = ((GraveurInfoMédia(médiaTailleUtilisée)*0n2352)/1024)/1024
      		FIN
      		CBM_NbRepFic = "0 piste"
      		// On défini l'exention des fichiers filtrés.
      		gsExtCh = "*.WAV"
      	AUTRE CAS
      		// pas d'autres cas
      FIN
      
      //Initialisation de la jauge
      JAUGE_ContenuTotal..BorneMin = 0
      si xCapacitétotal > 0 ALORS
      	JAUGE_ContenuTotal..BorneMax = arrondi(xCapacitétotal,2)
      sinon
      	// Si pas de CD/DVD par défaut au initialise à 702 Mo
      	JAUGE_ContenuTotal..BorneMax = 702
      FIN
      //Affichage de la place utilisé sur le Disque
      JAUGE_ContenuTotal..Valeur = arrondi(xEspaceUtilisé,2)
      
      // Affichage dans la barre de message
      CBM_Capité 				= "Espace total : "+arrondi(xCapacitétotal,2)+" Mo"
      CBM_EspaceOccupé 		= "Espace Occupé : "+arrondi(xEspaceUtilisé,2)+" Mo"
      CBM_EspaceTotalEstimer 	= "Espace Estimé : 0 Mo"
      
      // Rend actif les boutons de gravage actif
      FEN_Main.BAR_OutilsMain.BTN_FermerTiroir..etat 			= actif
      FEN_Main.BAR_OutilsMain.BTN_Ejecter..etat 				= actif
      FEN_Main.GrpCD..etat 									= Actif
      FEN_Main.GrpNouveau..etat 								= grisé
     type : 458752
