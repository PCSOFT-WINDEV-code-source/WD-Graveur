      procédure FEN_Compile(gNomCompile = "Compile1", gnTypeCompile est un entier = CompileDonnées)
      global
      	// Variables pour le remplissage de l'arbre1
      	gsBranche 				est une chaine			// Branche en cours de l'arbre1
      	gsCheminDeBase 			est une chaine			// Chemin de la branche de base
      	gsBrancheEnCours		est une chaine			// Chemin de la branche en cours d'utilisation
      	// Variables utiliser pour remplir l'arbre2 lors qu'un CD/DVD de données contient des données
      	gsBrancheCD_DVD 		est une chaine			// Branche en cours de l'arbre2
      	gsCheminDeBaseCD_DVD 	est une chaine			// Chemin de la branche de base du CD/DVD
      	gsBrancheEnCoursCD_DVD	est une chaine			// Chemin de la branche en cours d'utilisation du CD/DVD
      	// Variables du choix d'extension des fichiers à filtrer
      	gsExtCh		   			est une chaine = "*.*" 	// Extension à filtrer
      	// Autres variables pour les infos du disque
      	gxNbOctetEstimé 		est un numérique		// Nombre total d'octets estimé des fichiers à mettre sur CD/DVD
      	gxNbDeRepSurCD_DVD		est un numérique		// Nombre de répertoire sur futur CD/DVD
      	gxNbDeFichierSurCD_DVD 	est un numérique		// Nombre de fichier sur futur CD/DVD
      	xCapacitétotal 			est un numérique = 0	// Capacité total du disque
      	xEspacelibre 			est un numérique = 0	// Espace libre du disque
      	xEspaceUtilisé 			est un numérique = 0	// Espace occupé du disque
      	
      
      // Branchement des évènement pour le Drag&Drop entre les tables
      // Définition des sources et cible
      TABLE_Table1..DndSource = dndProgrammé
      TABLE_Table2..DndCible  = dndProgrammé
      TABLE_Audio..DndCible   = dndProgrammé
      
      // Définition des événements 
      DnDEvénement("OnDébutGlisser",TABLE_Table1,dndDébutGlisser)
      // Evenement de la table_table2
      DnDEvénement("OnSurvolTable2",TABLE_Table2,dndSurvol)
      DnDEvénement("ONLacherTable2",TABLE_Table2,dndLâcher)
      // Evenement de la table_Audio
      DnDEvénement("OnSurvolTableAudio",TABLE_Audio,dndSurvol)
      DnDEvénement("ONLacherTableAudio",TABLE_Audio,dndLâcher)
      
      // Création du fichier (si existe il est vidé)
      HCréation(FichierGrav)
