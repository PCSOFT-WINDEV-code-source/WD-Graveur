     name : _RemplirArbre
     internal_properties : CAAAAAgAAAC8quX/qhUuG/dsGm/b1HyaIY0w5tMlrBT9bi2KrGm5DRZsDtDL2++lL7qsEWz/50tstDINUEZHyTSAlMbJJ6sR96R6pSqls1nbaW/Zjhj49BSlGsRDvA0gMvMHjoJnqdFkLnHUd3rfYK+5USVdlXk70TmLUen0hFaunG9yaPmjmtiGVBcR/NIkE80r2CZ/CeqKI4WaL6bhQx2eM84kmYHrH0aYKz17W4wfwwOi55pMSqSIFomVkw/UzeO5Qc50moeer7M=
     procedure_id : 1447540563011671554
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //_RemplirArbre ( [<sNomArbre> est chaîne])
      //
      // Paramètres :
      //	sNomArbre (chaîne - valeur par défaut="ARBRE_Arbre1") : Nom de l'arbre à remplir
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //// Procédure automatique :
      // La procédure est exécutée manuellement, lors d'un appel dans le code
      // Elle s'exécutera dans un thread (sans avoir besoin d'appeler la fonction ThreadExecute), sans utilisation de HFSQL
      // 
      
      Procédure _RemplirArbre(sNomArbre est une chaine = "ARBRE_Arbre1")
      // Variables locales
      sLettreDisque 	est une chaine	// Lettre des disques (b:,c:,d:,...,z:)
      nInfoDisque 	est un entier	// Type de disque rencontré
      nI 				est un entier	// Variable de boucle
      sNomVol 		est une chaine	// Nom du volume
      
      
      // Racine de l'explorateur
      arbreajoute(sNomArbre,"Bureau",IMG_Bureau,IMG_Bureau,1)
      arbreajoute(sNomArbre,"Bureau"+TAB+"Mes Documents",IMG_Bureau,IMG_Bureau,completerep(sysrep(srMesDocuments)))
      arbreajoute(sNomArbre,"Bureau"+tab+"Poste de travail",IMG_Bureau,IMG_Bureau,2)
      
      // Passage de la branche en cours
      gsBranche = "Bureau"+TAB+"Mes Documents"
      // Liste les dossiers sur répertoire "\Mes documents"
      fListeRépertoire(sysrep(srMesDocuments) , _ListeRepertoire)
      
      //Parcours des lettre B à Z
      pour nI = 66 a 90
      	sLettreDisque = Caract(nI)+":"
      	nInfoDisque = fDisqueInfo(sLettreDisque,fdTypeDisque)
      	selon nInfoDisque
      		cas fdFixe
      			sNomVol = fDisqueInfo(sLettreDisque,fdNomVolume)
      			si  sNomVol = "" alors
      				sNomVol = "Disque Local"
      			fin
      			arbreajoute(sNomArbre,"Bureau"+tab+"Poste de travail"+tab+sLettreDisque+" "+sNomVol,IMG_Disque,IMG_Disque,Caract(nI)+":\")
      			// Passage de la branche en cours
      			gsBranche = "Bureau"+tab+"Poste de travail"+tab+sLettreDisque+" "+sNomVol
      			// Liste les dossiers sur répertoire "\Mes documents"
      			fListeRépertoire(ComplèteRep(sLettreDisque) , _ListeRepertoire)
      			listeajoute(COMBO_Combo1,gimage("LASER2-16-5.bmp")+" "+sLettreDisque+" "+sNomVol+glien(sLettreDisque+" "+sNomVol))
      		cas fdRéseau
      			arbreajoute(sNomArbre,"Bureau"+tab+"Poste de travail"+tab+sLettreDisque+" Disque Réseau",IMG_DisqueReseau,IMG_DisqueReseau,Caract(nI)+":\")
      			// Passage de la branche en cours
      			gsBranche = "Bureau"+tab+"Poste de travail"+tab+sLettreDisque+" Disque Réseau"
      			// Liste les dossiers sur répertoire "\Mes documents"
      			fListeRépertoire(ComplèteRep(sLettreDisque) , _ListeRepertoire)
      			listeajoute(COMBO_Combo1,gimage("SMP05051-16-5.png")+" "+sLettreDisque+" Disque Réseau"+glien(sLettreDisque+" Disque Réseau"))
      		cas fdCDRom
      			arbreajoute(sNomArbre,"Bureau"+tab+"Poste de travail"+tab+sLettreDisque+" CD/DVD-ROM",IMG_CDROM,IMG_CDROM,Caract(nI)+":\")
      			listeajoute(COMBO_Combo1,gimage("VST13202-16-5.png")+" "+sLettreDisque+" CD/DVD-ROM"+glien(sLettreDisque+" CD/DVD-ROM"))
      		AUTRE CAS
      			
      	FIN
      FIN
      // On déroule le bureau
      ArbreDéroule(ARBRE_Arbre1,"Bureau")
      // On sélectionne le disque c:
      ListeSelectplus(COMBO_Combo1,1)
      ExécuteTraitement(COMBO_Combo1,trtSélection)
