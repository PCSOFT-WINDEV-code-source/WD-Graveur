        code : |1-
         // suppression d'une piste
         nI est un entier = TableSelect(TABLE_Audio)
         
         nbSelectionné 	est un entier
         i 				est un entier
         nIndiceEnCours  est un entier
         
         
         // Suppression d'un fichier
         si _EVE.wParam = VK_DELETE ALORS
         	nbSelectionné = TableSelectOccurrence(TABLE_Audio)
         	POUR i = 1 A nbSelectionné
         		nIndiceEnCours = TableSelect(TABLE_Audio,i)
         		// Recherche dans le fichier sur une clé composée
         		gxNbOctetEstimé -= TABLE_Audio.COL_Taille[nIndiceEnCours]
         		gxNbDeFichierSurCD_DVD--
         	FIN
         	si nbSelectionné = TABLE_Audio..Occurrence alors 
         		TableSupprimeTout(TABLE_Audio)
         	sinon
         		pour i = 1 _a_ nbSelectionné
         			nIndiceEnCours = TableSelect(TABLE_Audio,i)
         			TableSupprime(TABLE_Audio,nIndiceEnCours)
         		FIN
         		pour i = 1 _à_ TABLE_Audio..Occurrence
         			TABLE_Audio.COL_Piste[i] = i
         		FIN
         	FIN
         	
         	// Mise a jour des données de la barre de message
         	AffichageBarreMessage("Supp")
         FIN
        type : 65792
