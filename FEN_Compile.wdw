     name : AffichageBarreMessage
     procedure_id : 1449447120466176383
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //AffichageBarreMessage ( [<mode> est chaîne])
      //
      // Paramètres :
      // 	mode (chaîne - valeur par défaut="Ajout") : <indiquez ici le rôle de mode>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      Procédure AffichageBarreMessage(sMode est une chaine = "Ajout")
      
      // Nombre de Mo estimé sur le disque
      CBM_EspaceTotalEstimer = "Espace Total estimé : "+NumériqueVersChaîne(arrondi((gxNbOctetEstimé/1048576),2))+" Mo"
      
      si sMode = "Ajout" alors
      	// Ajout visuel de la place futur prise sur le disque
      	JAUGE_ContenuTotal..Valeur = xEspaceUtilisé + arrondi((gxNbOctetEstimé/1048576),2)
      sinon
      	JAUGE_ContenuTotal..Valeur = xEspaceUtilisé + arrondi((gxNbOctetEstimé/1048576),2)
      FIN
      
      selon gnTypeCompile
      	cas CompileDonnées
      		// Affichage du nombre de répertoires et fichiers
      		si gxNbDeRepSurCD_DVD > 1 ALORS
      			si gxNbDeFichierSurCD_DVD > 1 ALORS
      				CBM_NbRepFic = NumériqueVersChaîne(gxNbDeRepSurCD_DVD)+" répertoires et "+NumériqueVersChaîne(gxNbDeFichierSurCD_DVD)+" fichiers"
      			sinon
      				CBM_NbRepFic = NumériqueVersChaîne(gxNbDeRepSurCD_DVD)+" répertoires et "+NumériqueVersChaîne(gxNbDeFichierSurCD_DVD)+" fichier"
      			FIN
      			
      		sinon
      			si gxNbDeFichierSurCD_DVD > 1 ALORS
      				CBM_NbRepFic = NumériqueVersChaîne(gxNbDeRepSurCD_DVD)+" répertoire et "+NumériqueVersChaîne(gxNbDeFichierSurCD_DVD)+" fichiers"
      			sinon
      				CBM_NbRepFic = NumériqueVersChaîne(gxNbDeRepSurCD_DVD)+" répertoire et "+NumériqueVersChaîne(gxNbDeFichierSurCD_DVD)+" fichier"
      			FIN
      			
      		FIN
      	cas CompileAudio
      		si gxNbDeFichierSurCD_DVD > 1 ALORS
      			CBM_NbRepFic = NumériqueVersChaîne(gxNbDeFichierSurCD_DVD)+" pistes"
      		sinon
      			CBM_NbRepFic = NumériqueVersChaîne(gxNbDeFichierSurCD_DVD)+" piste"
      		FIN
      	AUTRE CAS
      		
      FIN
