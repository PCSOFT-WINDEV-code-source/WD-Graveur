        code : |1+
         nbSelectionné 		est un entier
         i 					est un entier = 1
         nIndiceEnCours  	est un entier
         
         
         // Suppression d'un fichier
         si _EVE.wParam = VK_DELETE ALORS
         	nbSelectionné = TableSelectOccurrence(TABLE_Table2)
         	
         	tantque i <= nbSelectionné
         		nIndiceEnCours = TableSelect(TABLE_Table2,i)
         		si nIndiceEnCours <> -1 ALORS
         			// Recherche dans le fichier sur une clé composée
         			HLitRecherche(FichierGrav,CheminCompletCheminArbre,[TABLE_Table2.COL_CheminCompletDD[nIndiceEnCours],ArbreSelect(ARBRE_Arbre2)])
         			si htrouve() ALORS
         				// Si le fichier n'est pas déjà gravé
         				si pas FichierGrav.DéjaGravé alors
         					i++ // un fichier en plus
         					HSupprime(FichierGrav)
         					gxNbOctetEstimé -= COL_Taille[nIndiceEnCours]
         					gxNbDeFichierSurCD_DVD--
         				sinon
         					// déselectionne le fichier
         					TableSelectMoins(TABLE_Table2,nIndiceEnCours)
         					// mise à jour du nombre de fichiers sélectionnés
         					nbSelectionné = TableSelectOccurrence(TABLE_Table2)
         					si i > 1 alors i-- sinon i=1
         				FIN
         			FIN
         		FIN
         	FIN
         	pour i = 1 a nbSelectionné
         		nIndiceEnCours = TableSelect(TABLE_Table2,i)
         		TableSupprime(TABLE_Table2,nIndiceEnCours)
         	FIN
         	// Mise a jour des données de la barre de message
         	AffichageBarreMessage("Supp")
         FIN
         
        type : 65792
