     name : ONLacherTable2
     internal_properties : CAAAAAgAAADF/p3rj9YYYg9RUK5UCVypboGeNG2kWBbyPldr2uE2UNY9qhLUZRX0u4HEpAh8RX7SePLUE3tGYSk1qsuLJWtxfF+Kc+UeIQeZH7b6F9J5C1Y5VWw13mTQeULsdZBxsjy7TXUV1SCVgWoh7joDHaXjDajljm3IZjyaE6DvKDZ755vBbpk5r/KdIELbcOgQ75Sv/7CASIbjch7ynwPOrUGydEld+k9bn2nuttWQeuhBwrUtNBYi
     procedure_id : 1447580897052385237
     type_code : 14
     code : |1+
      // Syntaxe :
      //ONLacherTable2 ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      Procédure ONLacherTable2()
      sRes 		est une chaine	// Fichier ou répertoire copié
      sChCom 		est une chaine	// Chemin d'accès du fichier ou répertoire
      sext 		est une chaine	// Extension du fichier
      sNomRepFic 	est une chaine	// Nom du répertoire copié
      sFic		est une chaine	// Résultat des éléments copiés (Drag&Drop) (liste des fichiers et/ou répertoire
      
      
      sFic = DnDRécupère(CF_TEXT)
      
      POUR TOUTE CHAINE sRes DE sFic SEPAREE PAR rc
      	// Extension du fichier
      	sext 		= ExtraitChaîne(sRes,2,tab)
      	// Chemin d'accès complet au fichier
      	sChCom 		= ExtraitChaîne(sRes,5,tab)
      	// Nom du répertoire Copié
      	sNomRepFic 	= milieu(sRes,Position(sRes,"]",1)+1)
      	sNomRepFic 	= sansespace(ExtraitChaîne(sNomRepFic,1,tab))
      	
      	// On garde la branche sélectionne
      	si ArbreSelect(ARBRE_Arbre2) <> "" ALORS
      		gsBranche 		= ArbreSelect(ARBRE_Arbre2)
      	sinon
      		gsBranche 		= "Racine"
      	FIN
      	
      	// Recherche l'extension du fichier
      	si fExtraitChemin(sChCom,fExtension) <> "" ALORS
      		TableAjoute(TABLE_Table2,sRes)
      		// Ajout dans le fichier de données de l'arborescence.
      		hraz(FichierGrav)
      		FichierGrav.CheminArbre = gsBranche
      		FichierGrav.CheminComplet = sChCom
      		FichierGrav.DateFic       = fdate(sChCom,"",fModification)
      		FichierGrav.Nomfichier    = fExtraitChemin(sChCom,fFichier)
      		FichierGrav.TailleFic	  = fTaille(sChCom)
      		FichierGrav.Exten		  = fExtraitChemin(sChCom,fExtension)
      		hajoute(FichierGrav)
      		// Calcul de l'espace utilisé
      		gxNbOctetEstimé += FichierGrav.TailleFic
      		// Ajout d'un fichier supplémentaire au compteur
      		gxNbDeFichierSurCD_DVD++
      	sinon
      		// Il s'agit d'un répertoire. On créé les branches
      		// CD/DVD de Données On créer une branche et on liste tous les fichiers du rep.
      		TableSupprimeTout(TABLE_Table2)
      		si sext = "" ALORS // c'est un répertoire
      			// Récupère le chemin d'accès du Répertoire
      			gsCheminDeBase = ExtraitChaîne(sChCom,1,caract(0))
      			// Construction de la branche de Départ
      			gsBranche += tab + sNomRepFic
      			// La branche en cours est identique à la première branche de l'élément copié (au départ)
      			gsBrancheEnCours = gsBranche
      			// Ajout de la première branche
      			arbreAjoute(ARBRE_Arbre2,gsBranche)
      			// Liste des Répertoires et fichiers du repertoire copié
      			fListeFichier(ExtraitChaîne(sChCom,1,Caract(0))+"*.*",_ListeFicEtRepAajouter)
      		FIN	
      	FIN
      FIN
      
      // On déroule l'arbre et selection de la première branche créée
      ArbreDéroule(ARBRE_Arbre2,gsBranche)
      ArbreSelectPlus(ARBRE_Arbre2,gsBranche)
      ExécuteTraitement(ARBRE_Arbre2,trtSélection)
      
      // Affichage des infos dans la barre de message
      AffichageBarreMessage()
      
      
      
