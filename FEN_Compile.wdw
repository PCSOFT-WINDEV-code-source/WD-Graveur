      -
        code : |1+
         //Affichage des fichier disponible sur le futur CD/DVD
         schemin 		est une chaine = ArbreSelect(ARBRE_Arbre2)
         sCléParcours 	est une chaine
         sCondition 		est une chaine
         nIndiceLigne     est un entier
         
         
         // Supprime le contenu de la table
         TableSupprimeTout(TABLE_Table2)
         // Condition du Filtre
         sCondition=ChaîneConstruit("CheminArbre = '%1' ",schemin)
         // Recherche dans le fichier
         sCléParcours = hfiltre(FichierGrav,sCondition)
         HLitPremier(FichierGrav,sCléParcours)
         tantque pas HEnDehors()
         	nIndiceLigne++
         	tableajouteligne(TABLE_Table2,FichierGrav.Nomfichier,FichierGrav.Exten,FichierGrav.TailleFic,FichierGrav.DateFic,FichierGrav.CheminComplet)
         	si pas FichierGrav.DéjaGravé ALORS
         		TABLE_Table2[nIndiceLigne]..Couleur = iNoir
         	sinon
         		TABLE_Table2[nIndiceLigne]..Couleur = iGrisClair
         	FIN
         	HLitSuivant(FichierGrav,sCléParcours)
         FIN
         
         
         
         
        type : 33
      -
        type : 17
        enabled : false
      -
        internal_properties : CAAAAAgAAADj1fohfb4+Jr98v5rcJ9hZm61snGgt8xOHatv4yAUPV9EU9sCbOFlN2LOTlLD+hS7/SupgEFRGiBohRFl0/IroQSVYr+m43KwAol6oio5Ejj8NDDd+5yqrLCy5EAvtQ+jZqXt1Vo/2yowu34Q=
        code : |1+
         // Suppression d'une Branche et Son contenu
         schemin 		est une chaine = ArbreSelect(ARBRE_Arbre2)
         sCléParcours 	est une chaine
         sCondition 		est une chaine
         
         si _EVE.wParam = VK_DELETE ALORS
         	// Suppression uniquement des branche en noir (les branches en gris clair sont déjà gravé)
         	si ARBRE_Arbre2[schemin]..couleur <> iGrisClair ALORS
         		si schemin <> "Racine" ALORS
         			schemin 		= ArbreSelect(ARBRE_Arbre2)
         			si ARBRE_Arbre2[schemin]..Couleur = iGrisClair alors info("Déjà gravé")
         			sCondition		= ChaîneConstruit("CheminArbre ]= '%1' ",schemin)
         			sCléParcours 	= hfiltre(FichierGrav,sCondition)
         			gxNbDeRepSurCD_DVD--
         			HLitPremier(FichierGrav,sCléParcours)
         			tantque pas HEnDehors()
         				gxNbDeFichierSurCD_DVD--
         				gxNbOctetEstimé -= FichierGrav.TailleFic
         				HSupprime(FichierGrav)
         				HLitSuivant(FichierGrav,sCléParcours)
         			FIN
         			TableSupprimeTout(TABLE_Table2)
         			ArbreSupprime(ARBRE_Arbre2,schemin)
         		FIN
         	FIN
         FIN
         
         //Mise à jour de l'affichage de la barre de message
         AffichageBarreMessage("Supp")
