        code : |1+
         // Faire remonter une piste
         indice est un entier = TableSelect(TABLE_Audio)	//indice de la piste selectionnée
         schaineDépart est une chaine					// Lensemble des éléments du tableau à déplacer
         
         si indice <> -1 _et_ indice-1 <> 0 ALORS
         	schaineDépart = TABLE_Audio.COL_Nom[indice]+tab+TABLE_Audio.COL_Time[indice]+tab+TABLE_Audio.COL_Taille[indice]+tab+TABLE_Audio.COL_CheminD_accès[indice]
         	// Déplacement de la ligne du dessous à la ligne en cours
         	TableModifie(TABLE_Audio,indice+tab+TABLE_Audio.COL_Nom[indice-1]+tab+TABLE_Audio.COL_Time[indice-1]+tab+TABLE_Audio.COL_Taille[indice-1]+tab+TABLE_Audio.COL_CheminD_accès[indice-1],indice)
         	// Déplacement de la ligne sauvegardée à la ligne du dessous
         	TableModifie(TABLE_Audio,indice-1+tab+schaineDépart,indice-1)
         	// selection de la même ligne déplacée
         	TableSelectPlus(TABLE_Audio,indice-1)
         FIN
        type : 18
